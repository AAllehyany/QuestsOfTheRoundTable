package group52.comp3004.controllers;

import javafx.fxml.FXMLLoader;
import javafx.scene.layout.Pane;

public class PortraitAreaController {
Pane portrait;
	
	//Constructor
	public PortraitAreaController() throws Exception{
		FXMLLoader loader = new FXMLLoader(getClass().getResource("/fxml/portrait_area.fxml"));
		portrait = loader.load();
	}
}
